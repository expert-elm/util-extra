{
  "name": "util-extra",
  "version": "0.0.23",
  "description": "utils",
  "main": "cjs/index.js",
  "module": "module/index.mjs",
  "scripts": {
    "start": "tsc -w",
    "test": "jest",
    "prebuild": "rm dist -rf",
    "build": "ts-node build.ts",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist",
    "prerelease": "npm run build && npm version patch && cp package.json dist",
    "release": "cd dist && npm publish"
  },
  "files": [
    "**/*.js",
    "**/*.js.map",
    "**/*.mjs",
    "**/*.mjs.map",
    "**/*.ts",
    "**/*.d.ts",
    "**/*.d.ts.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expert-elm/util-extra.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expert-elm/util-extra/issues"
  },
  "homepage": "https://github.com/expert-elm/util-extra#readme",
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^10.14.10",
    "glob": "^7.1.4",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.1.1",
    "ts-jest": "^24.1.0",
    "ts-morph": "^2.3.0",
    "ts-node": "^8.3.0",
    "ts-repack": "0.0.9",
    "typescript": "^3.7.0-beta"
  },
  "sideEffects": false,
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/test/",
      "<rootDir>/node_modules/"
    ],
    "verbose": true
  },
  "dependencies": {
    "tslib": "^1.10.0"
  }
}
